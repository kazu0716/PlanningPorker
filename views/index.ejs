  <body id="body-main" style="padding-top: 0px">
    <div class="modal hide" id="name_in">
      <div class="modal-header">
        <h3>PlanningPorker</h3>
      </div>
      <div class="modal-body">
        <p>Please type your name.</p>
        <form id="login_form">
          <input type="text" id="login_name" class="span2" placeholder="Name"/>
        </form>
      </div>
      <div class="modal-footer">
        <button id="login" class="btn btn-primary">Login</button>
      </div>
    </div>

    <div class="navbar navbar-default navbar-fixed-top" role="navigation">
      <a class="navbar-brand" href="#">PlanningPorker</a>
      <form class="navbar-form navbar-left" id="form" role="search">
        <div class="form-group">
          <input type="text" name="name" id="name" class="form-control" placeholder="Name">
        </div>
      </form>
    </div>

    <div class="row">
      <div id="chat_area" class="col-xs-12">
        <div class="area-inner">
          <div class="dummy-top-space"></div>
          <img id="login_list_loader" src="/img/loader.gif"/>
          <div id="login_list" style="display:none"></div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="area-inner">
          <div class="number-list">
            <ul>
              <li><button type="button" class="btn btn-default">0</button></li>
              <li><button type="button" class="btn btn-default">1</button></li>
              <li><button type="button" class="btn btn-default">2</button></li>
              <li><button type="button" class="btn btn-default">3</button></li>
              <li><button type="button" class="btn btn-default">5</button></li>
              <li><button type="button" class="btn btn-default">8</button></li>
              <li><button type="button" class="btn btn-default">10</button></li>
              <li><button type="button" class="btn btn-default">Big</button></li>
              <li><button type="button" class="btn btn-default">?</button></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div id="memo_area" class="col-xs-12 memo-area">
        <div class="area-inner">
          <div id="share-memo">
          </div>
          <footer>
          <span>Â© 2014 Naoki KODAMA. All rights reserved.</span>
          </footer>
        </div>
      </div>
    </div>
  </body>
